@model IEnumerable<WebApplication2.Models.Patient>

<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th></th>
    </tr>
    @foreach (var patient in Model)
    {
        <tr>
            <td>@patient.Id</td>
            <td>@patient.Name</td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = patient.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = patient.Id })
            </td>
        </tr>
    }
</table>